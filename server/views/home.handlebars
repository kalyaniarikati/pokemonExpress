<nav class="navbar navbar-dark bg-dark">
    <a class="navbar-brand" href="#">Pokemon Team</a>
</nav>

<div>
    <div class="row justify-content-center">
                {{#each pokeObj}}
        <div class="col-md-2" style="margin-top: 50px;">
            <div class="card" >
                <img src="{{this.image}}"
                    class="card-img-top" style="width: 10vw; height:10vh" alt="...">
                <div class="card-body">
                    <h5 class="card-title">{{this.name}}</h5>

                     {{#if this.desc}}
                    <ul class="list-group">
                        {{#each poke.desc}}
                        <li class="list-group-item">{{this.desc}}</li>
                        {{/each}}
                    </ul>
                    {{else}}
                    <ul class="list-group">
                        <li class="list-group-item">Pokemon Character</li>
                    </ul>
                    {{/if}}
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>
